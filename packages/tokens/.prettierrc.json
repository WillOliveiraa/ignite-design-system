{
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none",
    "endOfLine": "auto",
    "importOrder": [
        "^react$",
        "^next$",
        "^[a-z]",
        "^[@]",
        "^[../]",
        "^[./]",
        "^.+\\.s?css$",
        "<THIRD_PARTY_MODULES>"
    ],
    "importOrderSeparation": true
}
